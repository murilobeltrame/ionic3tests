        "jasmine-spec-reporter": "^4.1.0",
        "karma": "^1.5.0",
        "karma-chrome-launcher": "^2.0.0",
        "karma-jasmine": "^1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-webpack": "^2.0.3",
        "null-loader": "^0.1.1",
        "protractor": "^5.1.1",
        "ts-loader": "^2.0.3",
        "ts-node": "^3.0.2",
        "typescript": "~2.8.0"
